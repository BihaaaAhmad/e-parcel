<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <display-name>E-parcel Application</display-name>
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>com.controller.AdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentController</servlet-name>
        <servlet-class>com.controller.StudentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaffController</servlet-name>
        <servlet-class>com.controller.StaffController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParcelDao</servlet-name>
        <servlet-class>Parcel.ParcelDao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Parcel</servlet-name>
        <servlet-class>Parcel.Parcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewParcel</servlet-name>
        <servlet-class>com.parcel.ViewParcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditParcel1</servlet-name>
        <servlet-class>com.parcel.EditParcel1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditParcel2</servlet-name>
        <servlet-class>com.parcel.EditParcel2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveParcel</servlet-name>
        <servlet-class>com.parcel.SaveParcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteParcel</servlet-name>
        <servlet-class>com.parcel.DeleteParcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentParcel</servlet-name>
        <servlet-class>com.parcel.StudentParcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>receiptPage</servlet-name>
        <servlet-class>receiptPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaffParcel</servlet-name>
        <servlet-class>com.parcel.StaffParcel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentController</servlet-name>
        <url-pattern>/StudentController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaffController</servlet-name>
        <url-pattern>/StaffController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParcelDao</servlet-name>
        <url-pattern>/ParcelDao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Parcel</servlet-name>
        <url-pattern>/Parcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewParcel</servlet-name>
        <url-pattern>/ViewParcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditParcel1</servlet-name>
        <url-pattern>/EditParcel1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditParcel2</servlet-name>
        <url-pattern>/EditParcel2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveParcel</servlet-name>
        <url-pattern>/SaveParcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteParcel</servlet-name>
        <url-pattern>/DeleteParcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentParcel</servlet-name>
        <url-pattern>/StudentParcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>receiptPage</servlet-name>
        <url-pattern>/receiptPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaffParcel</servlet-name>
        <url-pattern>/StaffParcel</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
